    <script type="text/javascript">
      $(document).ready(function() {
        $('#ei-slider').eislideshow({
          animation     : 'center',
          autoplay      : true,
          slideshow_interval  : 3000,
          titlesFactor    : 0
        });

        <!--STYCKY MENU-->
        $('#main-menu a').on({
          click: function(e) {
            e.preventDefault();
            $('body').stop().scrollTo(
              $( $(this).attr('href') ),
              500,
              {offset:-70}
            );
          }
        });

        <!--SKILLS ANIM-->
        $('#skills').waypoint({
          handler: function(event, direction) {
            $('.bar').each(function(e){
              $(this).animate({
                width: $(this).attr('data-width') + '%'
              }, {
                duration: 2000,
                easing: 'easeOutBounce'
              });
            });
          }
        });
       
        $('.showOlderChanges').on('click', function(e){
          $('.changelog .old').slideDown('slow');
          $(this).fadeOut();
          e.preventDefault();
        });

